- var general  = site.data.general
header
    .header-wrap
        .logo-group
            a.logo-link.site-track(href=url_for())
                img(src=url_for(theme.logo) alt="logo")
            span.conf-date #{date(general["beginning-date"], "MMMM D-" + date(general["ending-date"], "D") + ", YYYY")}
        .header-right
            .navigation
                include nav-main
                include nav-sub
                include nav-mobile
            include social-bar
            a.button.buy-tickets.site-track(href="/register")
                != general["header-register-button-text"]
