- general = site.data.general

.social-bar
    a(href=general["facebook"], alt="facebook")
        img(class="social-icons", src="/images/general/facebook.png")
    a(href=general["Twitter"], alt="Twitter")
        img(class="social-icons", src="/images/general/twitter.png")
    a(href=general["LinkedIn"], alt="LinkedIn")
        img(class="social-icons", src="/images/general/linkedin.png")
    a(href=general["YouTube"], alt="Youtube")
        img(class="social-icons", src="/images/general/youtube.png")
